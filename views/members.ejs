                <a href=/login/logout id="logoutBtn" class="btn btn-primary">ログアウト</a>
                <h2>Starting Member</h2>
                <form method="post" action="/scorebook">
                    <table class="table table-light">
                        <thead class="thead-dark">
                            <tr>
                                <th scope="col">打順</th>
                                <th scope="col">守備</th>
                                <th scope="col">氏名</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                    <input type="submit" class="btn btn-warning" value="登録"></input>
                </form>
            </div>
        </div>
    </div>

